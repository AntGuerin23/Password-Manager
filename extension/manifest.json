{
  "manifest_version": 3,
  "name": "SosPass",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "js": ["vendor/jquery.min.js", "scripts/app.js"]
  }],
  "action": {
    "default_popup" : "popup/popup.html",
    "default_icon" : "images/lock.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}