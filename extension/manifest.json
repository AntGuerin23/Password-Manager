{
  "manifest_version": 3,
  "name": "SosPass",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "js": ["vendor/jquery.min.js", "app.js"]
  }],
  "background" : {
    "service_worker" : "background.js"
  },
  "action": {
    "default_popup" : "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}